{
	"name": "vscode-language-cimpl",
	"description": "VSCode CIMPL Language Support",
	"author": "The MITRE Corporation",
	"license": "Apache-2.0",
	"version": "2.2.4",
	"publisher": "kmahalingam",
	"repository": {
		"type": "git",
		"url": "https://github.com/standardhealth/vscode-language-cimpl"
	},
	"engines": {
		"vscode": "^1.23.0"
	},
	"activationEvents": [
		"onLanguage:cimpl"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "cimpl",
				"aliases": [
					"CIMPL",
					"cimpl"
				],
				"extensions": [
					".txt",
					".cimpl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cimpl",
				"scopeName": "source.cimpl",
				"path": "./syntaxes/cimpl.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "cimpl",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.0",
		"@types/node": "^8.0.0",
		"typescript": "2.8.3",
		"vscode": "^1.1.18"
	},
	"dependencies": {
		"antlr4": "~4.6.0",
		"extend": ">=3.0.2",
		"node.extend": ">=1.1.7",
		"tar": ">=4.4.2"
	}
}
