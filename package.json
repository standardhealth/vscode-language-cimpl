{
    "name": "vscode-language-cimpl",
    "displayName": "vscode-language-cimpl",
    "description": "VSCode CIMPL Language Support",
    "version": "0.2.0",
    "repository": "https://gitlab.mitre.org/mokeefe/vscode-language-cimpl",
    "publisher": "okeefm",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cimpl",
                "aliases": [
                    "CIMPL",
                    "cimpl"
                ],
                "extensions": [
                    ".txt",
                    ".cimpl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cimpl",
                "scopeName": "source.cimpl",
                "path": "./syntaxes/cimpl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.37.5"
    }
}
